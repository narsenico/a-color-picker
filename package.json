{
  "name": "a-color-picker",
  "version": "1.2.2",
  "description": "a color pikcer for web app",
  "main": "./dist/acolorpicker.js",
  "types": "./dist/acolorpicker.d.ts",
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "build:dev": "webpack --display-used-exports --display-reasons --config webpack.dev.js",
    "build:docs": "webpack --config webpack.docs.js",
    "start:docs": "webpack-dev-server --open --open-page index.html --port 3000 --config webpack.docs.js",
    "test": "webpack-dev-server --open --open-page test.html --port 3000 --config webpack.dev.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/narsenico/a-color-picker.git"
  },
  "keywords": [
    "javascript",
    "color",
    "picker",
    "color-picker"
  ],
  "author": "narsenico",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/narsenico/a-color-picker/issues"
  },
  "homepage": "https://narsenico.github.io/a-color-picker/",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^5.5.0",
    "style-loader": "^0.21.0",
    "svg-url-loader": "^8.0.0",
    "text-loader": "0.0.1",
    "to-string-loader": "^1.1.5",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.3",
    "webpack-dev-server": "^3.7.1",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "is-plain-object": "^2.0.4"
  }
